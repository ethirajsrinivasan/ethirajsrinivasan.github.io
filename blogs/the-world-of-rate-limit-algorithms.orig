<div class="highlight highlight-source-python notranslate position-relative overflow-auto" dir="auto" data-snippet-clipboard-copy-content="import time

class RateLimiter:
    def __init__(self, max_requests, window_duration):
        self.max_requests = max_requests
        self.window_duration = window_duration
        self.window_start_time = time.time()
        self.requests_made = 0

    def allow_request(self):
        current_time = time.time()
        time_since_window_start = current_time - self.window_start_time

        if time_since_window_start &gt; self.window_duration:
            self.window_start_time = current_time
            self.requests_made = 0

        if self.requests_made &lt; self.max_requests:
            self.requests_made += 1
            return True
        else:
            return False"><pre><span class="pl-k">import</span> <span class="pl-s1">time</span>

